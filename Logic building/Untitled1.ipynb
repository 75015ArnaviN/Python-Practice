    "import math\n",
    "def is_prime(no):\n",
    "    flag= False\n",
    "    for i in range(1,int(math.sqrt(no))):\n",
    "        if no%i==0:\n",
    "            return flag\n",
    "        else:\n",
    "            flag= True\n",
    "            return flag\n",
    "\n",
    "    \n",
    "number=int(input(\"Enter a number:\"))\n",
    "is_prime(number)"
   ]
